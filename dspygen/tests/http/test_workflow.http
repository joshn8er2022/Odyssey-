{
  "yaml_content": "name: TransactionProcessingWorkflow\ntriggers: manual\njobs:\n  - name: TransactionAnalysis\n    runner: python\n    steps:\n      - name: InitializeTransactions\n        code: |\n          transactions = [\n            {'id': 1, 'amount': 250, 'category': 'Electronics'},\n            {'id': 2, 'amount': 75, 'category': 'Books'},\n            {'id': 3, 'amount': 150, 'category': 'Home Goods'},\n            {'id': 4, 'amount': 20, 'category': 'Apparel'},\n            {'id': 5, 'amount': 500, 'category': 'Electronics'}\n          ]\n          print('Transactions initialized.')\n\n      - name: FilterHighValueTransactions\n        code: |\n          global high_value_transactions\n          high_value_transactions = [txn for txn in transactions if txn['amount'] > 100]\n          print(f'High value transactions filtered: {high_value_transactions}')\n        if_:\n          expr: \"len(transactions) > 0\"\n\n      - name: PrintHighValueTransactionsDetails\n        code: |\n          for txn in high_value_transactions:\n              print(f\"Transaction ID: {txn['id']}, Amount: ${txn['amount']}, Category: {txn['category']}\")\n        loop:\n          over: \"high_value_transactions\"\n          var: \"txn\"\n\n      - name: CalculateSummaryStatistics\n        code: |\n          total_amount = sum(txn['amount'] for txn in high_value_transactions)\n          average_amount = total_amount / len(high_value_transactions)\n          print(f'Total amount from high value transactions: ${total_amount}')\n          print(f'Average amount from high value transactions: ${average_amount}')\n        if_:\n          expr: \"len(high_value_transactions) > 0\"\n\n      - name: Summary\n        code: |\n          print('Workflow execution completed. High value transaction analysis done.')",
  "init_ctx": {}
